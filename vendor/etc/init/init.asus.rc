on init

#create big-aff cpuset

    mkdir /dev/cpuset/big-aff
    chown system system /dev/cpuset/big-aff
    chown system system /dev/cpuset/big-aff/cpus
    chown system system /dev/cpuset/big-aff/tasks
    chmod 0660 /dev/cpuset/big-aff/cpus
    chmod 0660 /dev/cpuset/big-aff/tasks
    write /dev/cpuset/big-aff/cpus 4-7 
    write /dev/cpuset/big-aff/mems 0
    
on fs
chmod 660 /dev/kmsg

on post-fs
# ASUS_BSP : For Accy FW Check
start CheckAccyAsusfwVer

# mount factory
write /proc/bootprof "ASUS RC : mount /vendor/factory"
wait /dev/block/by-name/factory
exec /system/bin/e2fsck -p /dev/block/by-name/factory
mount ext4 /dev/block/by-name/factory /vendor/factory rw wait noatime
chown root system /vendor/factory
chmod 0771 /vendor/factory
restorecon_recursive /vendor/factory

